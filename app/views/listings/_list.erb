<table class="table table-striped table-hover">

  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= link_to listing.title, listing %></td>
        <td><%= listing.date_created %></td>
      </tr>
    <% end %>
  </tbody>
</table>